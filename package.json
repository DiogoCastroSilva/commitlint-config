{
  "name": "@diogozx-org/commitlint-config",
  "version": "1.0.0",
  "description": "An CommitLint config standard for my projects",
  "author": "Diogo Silva",
  "homepage": "https://github.com/DiogoCastroSilva/commitlint-config#readme",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "commit": "npx git-cz"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DiogoCastroSilva/commitlint-config.git"
  },
  "bugs": {
    "url": "https://github.com/DiogoCastroSilva/commitlint-config/issues"
  },
  "keywords": [
    "conventional-changelog",
    "commitlint",
    "commitlint-config"
  ],
  "engines": {
    "node": ">=v10"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
